    ============================================ -->
<script src="~/Utilities/js/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
<script src="~/Utilities/js/scrollbar/mCustomScrollbar-active.js"></script>
<!-- metisMenu JS
    ============================================ -->
<script src="~/Utilities/js/metisMenu/metisMenu.min.js"></script>
<script src="~/Utilities/js/metisMenu/metisMenu-active.js"></script>
<!-- morrisjs JS
    ============================================ -->
<script src="~/Utilities/js/morrisjs/raphael-min.js"></script>
<script src="~/Utilities/js/morrisjs/morris.js"></script>
<script src="~/Utilities/js/morrisjs/morris-active.js"></script>
<!-- morrisjs JS
    ============================================ -->
<script src="~/Utilities/js/sparkline/jquery.sparkline.min.js"></script>
<script src="~/Utilities/js/sparkline/jquery.charts-sparkline.js"></script>
<script src="~/Utilities/js/sparkline/sparkline-active.js"></script>
<!-- calendar JS
    ============================================ -->
<script src="~/Utilities/js/calendar/moment.min.js"></script>
<script src="~/Utilities/js/calendar/fullcalendar.min.js"></script>
<script src="~/Utilities/js/calendar/fullcalendar-active.js"></script>
<!-- plugins JS
    ============================================ -->
<script src="~/Utilities/js/plugins.js"></script>
<!-- main JS
    ============================================ -->
<script src="~/Utilities/js/main.js"></script>
<!-- tawk chat JS
    ============================================ -->
<script src="~/Utilities/js/tawk-chat.js"></script>
<!-- data table JS
    ============================================ -->
<script src="~/Utilities/js/data-table/bootstrap-table.js"></script>
<script src="~/Utilities/js/data-table/tableExport.js"></script>
<script src="~/Utilities/js/data-table/data-table-active.js"></script>
<script src="~/Utilities/js/data-table/bootstrap-table-editable.js"></script>
<script src="~/Utilities/js/data-table/bootstrap-editable.js"></script>
<script src="~/Utilities/js/data-table/bootstrap-table-resizable.js"></script>
<script src="~/Utilities/js/data-table/colResizable-1.5.source.js"></script>
<script src="~/Utilities/js/data-table/bootstrap-table-export.js"></script>



<div class="container body-content">
    @Html.Partial("_SweetAlert")


    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year - Mi aplicación ASP.NET</p>
    </footer>
</div>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)
</body>
</html>
